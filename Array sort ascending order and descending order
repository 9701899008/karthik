#include <stdio.h>
#include <stdlib.h>
int compare(const void *a,const void*b){
return(*(int*)a-*(int*)b);
}
int main(){
int arr[]={4, 2, 7, 1, 3};
int n = sizeof(arr) / sizeof(arr[0]);
qsort(arr, n, sizeof(int), compare);
printf("Array in ascending order: ");
for (int i = 0; i < n; i++) {
printf("%d ", arr[i]);
}
return 0;
}

output:
ascending order:1,2,3,4,7
descending order:7,4,3,2,1
